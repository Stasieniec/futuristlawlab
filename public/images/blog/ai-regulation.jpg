<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:0.3" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bg)"/>
  
  <!-- Network pattern -->
  <g opacity="0.3">
    <circle cx="200" cy="150" r="4" fill="#1e40af"/>
    <circle cx="400" cy="200" r="3" fill="#1e40af"/>
    <circle cx="600" cy="120" r="5" fill="#1e40af"/>
    <circle cx="800" cy="250" r="3" fill="#1e40af"/>
    <circle cx="1000" cy="180" r="4" fill="#1e40af"/>
    
    <line x1="200" y1="150" x2="400" y2="200" stroke="#1e40af" stroke-width="1"/>
    <line x1="400" y1="200" x2="600" y2="120" stroke="#1e40af" stroke-width="1"/>
    <line x1="600" y1="120" x2="800" y2="250" stroke="#1e40af" stroke-width="1"/>
    <line x1="800" y1="250" x2="1000" y2="180" stroke="#1e40af" stroke-width="1"/>
  </g>
  
  <!-- Main content -->
  <g>
    <!-- Title -->
    <text x="600" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="48" font-weight="bold" fill="#1e293b">
      AI Regulation
    </text>
    
    <!-- Subtitle -->
    <text x="600" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" fill="#475569">
      The Future of EU AI Act
    </text>
    
    <!-- Icon -->
    <g transform="translate(550, 380)">
      <rect x="0" y="0" width="100" height="80" rx="8" fill="#1e40af" opacity="0.2"/>
      <rect x="20" y="20" width="60" height="8" rx="4" fill="#1e40af"/>
      <rect x="20" y="35" width="40" height="8" rx="4" fill="#1e40af"/>
      <rect x="20" y="50" width="50" height="8" rx="4" fill="#1e40af"/>
    </g>
  </g>
  
  <!-- Logo area -->
  <text x="600" y="550" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#1e40af">
    Futurist Law Lab
  </text>
</svg>